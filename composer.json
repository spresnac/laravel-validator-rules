{
    "name": "spresnac/laravel-validator-rules",
    "license": "MIT",
    "description": "Some useful validator rules for laravel",
    "keywords": ["php", "laravel", "mime", "boolean", "valid-url"],
    "type": "library",
    "require": {
      "php": "^8.0|^8.1",
      "illuminate/contracts": "^8",
      "illuminate/support": "^8",
      "illuminate/validation": "^8"
    },
    "require-dev": {
      "phpunit/phpunit": "^9"
  },
    "autoload": {
      "psr-4": {
        "spresnac\\Rules\\": "src/spresnac/Rules/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "spresnac\\Rules\\tests\\": "tests/Rules/"
      }
    },
    "minimum-stability": "stable",
    "authors": [
      {
        "name": "Sascha Presnac",
        "email": "sascha@presnac.de"
      }
    ],
    "homepage": "https://github.com/spresnac/laravel-validator-rules",
    "support": {
      "issues": "https://github.com/spresnac/laravel-validator-rules/issues",
      "source": "https://github.com/spresnac/laravel-validator-rules"
    },
    "scripts": {
      "test-ci": "phpunit --coverage-clover coverage.xml"
    }
}
